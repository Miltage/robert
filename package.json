{
  "name": "robert",
  "version": "2.0.1",
  "description": "Personal site of Robert Shenton",
  "scripts": {
    "start": "npm run dev",
    "dev": "start http://localhost:8080 && cross-env ELEVENTY_ENV=development eleventy --config=eleventy.config.js --output=dist --serve",
    "mix": "webpack --watch --progress --stats-children --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js && eleventy --config=eleventy.config.js --output=docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Miltage/robert.git"
  },
  "author": "Robert Shenton",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "cross-env": "^7.0.2",
    "html-minifier": "^4.0.0",
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.7",
    "sass-loader": "^13.1.0",
    "tailwindcss": "^1.4.6",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^4.10.0"
  }
}
